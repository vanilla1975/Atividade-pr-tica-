#include <iostream>
#include<string>

using namespace std;

int main() {
    string str;
    cin>> str;
    
    int a=0;
    int e=0;
    int i=0;
    int o=0;
    int u=0;
    
    for(int j=0; j<str.size();j++){
        if(str[j]=='a'){
            a++;
        } 
        if(str[j]=='e'){
            e++;
        }
        if(str[j]=='i'){
            i++;
        }
        if(str[j]=='o'){
            o++;
        }
        if(str[j]=='u'){
            u++;
        }
    }
    
    if(a!=0){
      cout<< "a"<< " " << a << endl;
    }
    if(e!=0){
      cout<< "e"<< " " << e << endl;
    }
    if(i!=0){
      cout<< "i"<< " " << i << endl;
    }
    if(o!=0){
      cout<< "o"<< " " << o << endl;
    }
    if(u!=0){
      cout<< "u"<< " " << u << endl;
    }
    
    
    
    
    
    
    
    
  return 0;
}
